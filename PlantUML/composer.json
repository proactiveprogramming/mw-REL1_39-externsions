{
	"name": "pjkersten/plantuml-extension",
	"type": "library",
	"description": "MediaWiki extension which renders plantuml markup as UML diagramm graphics",
	"keywords": [
		"uml",
		"mediawiki"
	],
	"homepage": "https://github.com/pjkersten/PlantUML",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Pieter J. Kersten",
			"email": "p.j.kersten@edusense.nl",
			"homepage": "http://www.edusense.nl/",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"mediawiki/graph-viz": "*"
	},
	"autoload": {
		"psr-4": {
			"PlantUML\\": "src/"
		},
		"files" : [
			"PlantUML.php"
		],
		"classmap": [
			"tests/"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.0.6-dev"
		}
	},
	"replace": {
		"mediawiki/plantuml-extension": "*"
	}
}
