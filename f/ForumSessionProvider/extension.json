{
	"name": "ForumSessionProvider",
	"version": "1.1.0",
	"author": [
		"Vekseid",
		"jdarwood007"
	],
	"url": "https://github.com/Vekseid/ForumSessionProvider",
	"description": "Uses an Elkarte or SMF installation as a session provider for MediaWiki single-sign on.",
	"license-name": "BSD",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.29.0"
	},
	"config": {
		"Path": {
			"value": "../forum"
		},
		"NameStyle": {
			"value": "smf"
		},
		"Software": {
			"value": "elk1.1"
		},
		"Debug": {
			"value": false
		},
		"EnableBanCheck": {
			"value": false
		},
		"SuperGroups": {
			"value": []
		},
		"InterfaceGroups": {
			"value": []
		},
		"AdminGroups": {
			"value": []
		},
		"AllowGroups": {
			"value": []
		},
		"DenyGroups": {
			"value": []
		},
		"SpecialGroups": {
			"value": []
		}
	},
	"config_prefix": "wgFSP",
	"SessionProviders": {
		"ForumSessionProvider": {
			"class": "ForumSessionProvider",
			"args": []
		}
	},
	"AuthManagerAutoConfig": {
		"primaryauth": {
			"ForumAuthManager": {
				"class": "ForumAuthManager",
				"args": []
			}
		}
	},
	"DefaultUserOptions": {
		"forum_last_update_groups": 0,
		"forum_last_update_user": 0,
		"forum_last_update_ban": 0,
		"forum_member_id": 0,
		"forum_is_banned": 0
	},
	"Hooks": {
		"SpecialPageBeforeExecute": [
			"ForumSessionProvider::onSpecialPageBeforeExecute"
		]
	},
	"AutoloadClasses": {
		"ForumSessionProvider": "ForumSessionProvider.php",
		"ForumAuthManager": "ForumAuthManager.php"
	},
	"manifest_version": 2
}
