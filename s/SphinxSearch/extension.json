{
	"name": "SphinxSearch",
	"author": [
		"Svemir Brkic",
		"Paul Grinberg"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:SphinxSearch",
	"descriptionmsg": "sphinxsearch-desc",
	"license-name": "GPL-2.0",
	"type": "other",
	"version": "1.0.0",
	"requires": {
		"MediaWiki": ">= 1.31.0"
	},
	"AutoloadClasses": {
		"SphinxMWSearch": "SphinxMWSearch.php",
		"SphinxMWSearchResult": "SphinxMWSearchResult.php",
		"SphinxMWSearchResultSet": "SphinxMWSearchResultSet.php"
	},
	"ExtensionFunctions": [
		"SphinxMWSearch::initialize"
	],
	"MessagesDirs": {
		"SphinxSearch": [
			"i18n"
		]
	},
	"config": {
		"SphinxSearch_host": {
			"value": "127.0.0.1"
		},
		"SphinxSearch_port": {
			"value": 9312
		},
		"SphinxSearch_index": {
			"value": "wiki_main"
		},
		"SphinxSearch_index_list": {
			"value": "*"
		},
		"SphinxSearch_index_weights": {
			"value": "null"
		},
		"SphinxSearch_mode": {
			"value": 4
		},
		"SphinxSearch_sortmode": {
			"value": 0
		},
		"SphinxSearch_sortby": {
			"value": ""
		},
		"SphinxSearch_maxmatches": {
			"value": 1000
		},
		"SphinxSearch_cutoff": {
			"value": 0
		},
		"SphinxSearch_weights": {
			"value": {
				"old_text": 1,
				"page_title": 100
			}
		},
		"SphinxSearchMWHighlighter": {
			"value": false
		},
		"SphinxSuggestMode": {
			"value": ""
		},
		"SphinxSearchAspellPath": {
			"value": "aspell"
		},
		"SphinxSearchPersonalDictionary": {
			"value": ""
		},
		"EnableSphinxInfixSearch": {
			"value": false
		},
		"EnableSphinxPrefixSearch": {
			"value": false
		}
	},
	"manifest_version": 2
}
