{
	"name": "uLogin",
	"version": "1.2",
	"author": [
		"ipshenicyn",
		"kghbln",
		"UksusoFF",
		"ulogin-team"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:ULogin",
	"descriptionmsg": "Sign in with uLogin.",
	"license-name": "GPLv3",
	"type": "user identity",
	"AutoloadClasses": {
		"ULoginHooks": "ULogin.hooks.php",
		"SpecialHelloWorld": "specials/SpecialHelloWorld.php"
	},
	"config": {
		"ULoginProviders": {
			"value": "vkontakte,odnoklassniki,mailru,facebook,twitter,google,yandex,livejournal,openid,lastfm,linkedin,soundcloud"
		},
		"ULoginHidden": {
			"value": "other"
		},
		"ULoginDisplay": {
			"value": "small"
		},
		"ULoginSort": {
			"value": "relevant"
		},
		"ULoginTheme": {
			"value": "classic"
		}
	},
	"ExtensionMessagesFiles": {
		"ULoginAlias": "ULogin.i18n.alias.php"
	},
	"Hooks": {
		"UserLoginForm": [
			"ULoginHooks::onUserLoginForm"
		],
		"UserLoadFromSession": [
			"ULoginHooks::onUserLoadFromSession"
		]
	},
	"MessagesDirs": {
		"ULogin": [
			"i18n"
		]
	},
	"manifest_version": 2,
	"requires": {
		"MediaWiki": ">= 1.29.0"
	}
}
