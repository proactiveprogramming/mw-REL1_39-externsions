{
	"name": "Amp",
	"version": "0.0.0",
	"author": [
		"Nischayn22"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Amp",
	"license-name": "MIT",
	"type": "other",
	"AutoloadClasses": {
		"Amp": "Amp.php",
		"RecursiveDOMIterator": "RecursiveDOMIterator.php"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"Amp::onParserSetup"
		],
		"BeforePageDisplay": [
			"Amp::onBeforePageDisplay"
		],
		"PageContentSaveComplete": [
			"Amp::onPageContentSaveComplete"
		]
	},
	"config": {
		"AllPagesAmp": {
			"value": false
		},
		"AmpFooterLinks": {
			"value": {
				"left": [],
				"right": []
			}
		}
	},
	"manifest_version": 2,
	"requires": {
		"MediaWiki": ">= 1.29.0"
	}
}
