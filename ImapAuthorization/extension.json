{
	"name": "ImapAuthorization",
	"type": "other",
	"version": "0.1.1",
	"license-name": "MIT",
	"descriptionmsg": "imapauthorization-desc",
	"author": "Alphanoob1337",
	"url": "https://www.mediawiki.org/wiki/Extension:ImapAuthorization",
	"requires": {
		"MediaWiki": ">= 1.29.0"
	},
	"AuthManagerAutoConfig": {
		"primaryauth": {
			"ImapAuthenticationProvider": {
				"class": "ImapAuthenticationProvider"
			}
		}
	},
	"config": {
		"ImapAuthorizationActive": {
			"value": false
		},
		"ImapAuthorizationSmtpServerAddress": {
			"value": "localhost"
		},
		"ImapAuthorizationSmtpServerPort": {
			"value": 25
		},
		"ImapAuthorizationImapServerAddress": {
			"value": "localhost"
		},
		"ImapAuthorizationImapServerPort": {
			"value": 143
		},
		"ImapAuthorizationImapServerEnforceSsl": {
			"value": false
		},
		"ImapAuthorizationImapServerEnforceTls": {
			"value": false
		},
		"ImapAuthorizationImapServerVerifyCert": {
			"value": true
		}
	},
	"MessagesDirs": {
		"ImapAuthorization": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"ImapAuthenticationProvider": "ImapAuthenticationProvider.php"
	},
	"manifest_version": 2
}
