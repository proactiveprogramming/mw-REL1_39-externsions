{
	"name": "HideSidebar",
	"author": "Jlerner, MyWikis LLC",
	"url": "https://github.com/mywikis/HideSidebar",
	"description": "Hides sidebar from anonymous users",
	"version": "2.1.1",
	"license-name": "GPL-2.0",
	"type": "other",
	"manifest_version": 2,
	"config": {
		"EnableSidebarCache": {
			"value": {
				"value": false
			}
		}
	},
	"Hooks": {
		"SkinBuildSidebar": "HideSidebarHooks::efHideSidebar"
	},
	"AutoloadClasses": {
		"HideSidebarHooks": "HideSidebar.hooks.php"
	},
	"requires": {
		"MediaWiki": ">= 1.29.0"
	}
}
