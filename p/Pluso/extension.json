		"PlusoBackgroundHeader": {
			"value": "transparent"
		},
		"PlusoOptionsHeader": {
			"value": [
				"small",
				"square",
				"line",
				"horizontal",
				"counter"
			]
		},
		"PlusoThemeHeader": {
			"value": "08"
		},
		"PlusoServicesHeader": {
			"value": "vkontakte,odnoklassniki,facebook,twitter,livejournal,moimir"
		}
	},
	"ExtensionMessagesFiles": {
