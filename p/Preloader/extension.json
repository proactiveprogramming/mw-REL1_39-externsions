{
	"name": "Preloader",
	"version": "1.4",
	"author": [
		"Rob Church",
		"Troy Engel",
		"Jack Phoenix"
	],
	"license-name": "MIT",
	"url": "https://www.mediawiki.org/wiki/Extension:Preloader",
	"descriptionmsg": "preloader-desc",
	"type": "parserhook",
	"config": {
		"PreloaderSource": {
			"value": [
				"Template:Preload"
			],
			"merge_strategy": "array_plus"
		}
	},
	"MessagesDirs": {
		"Preloader": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"Preloader": "Preloader.class.php"
	},
	"Hooks": {
		"EditFormPreloadText": "Preloader::mainHook"
	},
	"manifest_version": 2,
	"requires": {
		"MediaWiki": ">= 1.29.0"
	}
}
