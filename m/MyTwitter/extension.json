{
	"name": "MyTwitter",
	"version": "1.0",
	"author": [
		"[http://www.axllent.org/code/mytwit/ Ralph Slooten]",
		"페네트-"
	],
	"url": "http://wiki-chan.net/",
	"description": "Show twitter widget in mediawiki, using Ralph's MyTwit class.",
	"license-name": "MIT",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.29.0"
	},
	"ExtensionFunctions": [
		"MyTwitWrapper::registerTwitterTag"
	],
	"AutoloadClasses": {
		"MyTwitWrapper": "MyTwitter.class.php",
		"MyTwit": "mytwit.inc.php"
	},
	"config": {
		"TwitterUser": {
			"value": ""
		},
		"TwitterConsumerKey": {
			"value": ""
		},
		"TwitterConsumerSecret": {
			"value": ""
		},
		"TwitterOAUTHAccessToken": {
			"value": ""
		},
		"TwitterOAUTHAccessTokenSecret": {
			"value": ""
		},
		"TwitterCacheExpire": {
			"value": 600
		},
		"TwitterPostLimit": {
			"value": 5
		},
		"TwitterExcludeReplies": {
			"value": true
		},
		"TwitterOpenLinksInBlank": {
			"value": true
		}
	},
	"manifest_version": 2
}
