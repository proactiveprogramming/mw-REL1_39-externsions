{
	"name": "LinkTitles",
	"author": [
		"[https://www.mediawiki.org/wiki/User:Bovender Daniel Kraus (bovender)]",
		"Adrian (bluedreamer)",
		"j-zero",
		"Brent Laabs (labster)",
		"Caleb Mingle (dentafrice)",
		"paladox",
		"Ulrich Strauss (c0nnex)",
		"tetsuya-zama",
		"yoshida"
	],
	"type": "parserhook",
	"url": "https://www.mediawiki.org/wiki/Extension:LinkTitles",
	"version": "8.1.0",
	"license-name": "GPL-2.0+",
	"descriptionmsg": "linktitles-desc",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"config": {
		"LinkTitlesParseOnEdit": {
			"value": true
		},
		"LinkTitlesParseOnRender": {
			"value": false
		},
		"LinkTitlesParseHeadings": {
			"value": false
		},
		"LinkTitlesSkipTemplates": {
			"value": true
		},
		"LinkTitlesPreferShortTitles": {
			"value": true
		},
		"LinkTitlesCheckRedirect": {
			"value": true
		},
		"LinkTitlesEnableNoTargetMagicWord": {
			"value": false
		},
		"LinkTitlesMinimumTitleLength": {
			"value": 4
		},
		"LinkTitlesBlackList": {
			"value": []
		},
		"LinkTitlesFirstOnly": {
			"value": true
		},
		"LinkTitlesSmartMode": {
			"value": true
		},
		"LinkTitlesWordStartOnly": {
			"value": true
		},
		"LinkTitlesWordEndOnly": {
			"value": true
		},
		"LinkTitlesSpecialPageReloadAfter": {
			"value": 1
		},
		"LinkTitlesSourceNamespaces": {
			"value": []
		},
		"LinkTitlesTargetNamespaces": {
			"value": []
		},
		"LinkTitlesSameNamespace": {
			"value": true
		}
	},
	"AutoloadClasses": {
		"LinkTitles\\Extension": "includes/Extension.php",
		"LinkTitles\\Linker": "includes/Linker.php",
		"LinkTitles\\Source": "includes/Source.php",
		"LinkTitles\\Target": "includes/Target.php",
		"LinkTitles\\Targets": "includes/Targets.php",
		"LinkTitles\\Splitter": "includes/Splitter.php",
		"LinkTitles\\Config": "includes/Config.php",
		"LinkTitles\\Special": "includes/Special.php",
		"LinkTitles\\TestCase": "tests/phpunit/TestCase.php"
	},
	"SpecialPages": {
		"LinkTitles": "LinkTitles\\Special"
	},
	"AvailableRights": [
		"linktitles-batch"
	],
	"GroupPermissions": {
		"sysop": {
			"linktitles-batch": true
		}
	},
	"Hooks": {
		"MultiContentSave": [
			"LinkTitles\\Extension::onMultiContentSave"
		],
		"InternalParseBeforeLinks": [
			"LinkTitles\\Extension::onInternalParseBeforeLinks"
		],
		"GetDoubleUnderscoreIDs": [
			"LinkTitles\\Extension::onGetDoubleUnderscoreIDs"
		],
		"ParserFirstCallInit": [
			"LinkTitles\\Extension::onParserFirstCallInit"
		]
	},
	"ExtensionMessagesFiles": {
		"LinkTitlesMagic": "includes/Magic.php"
	},
	"MessagesDirs": {
		"LinkTitles": [
			"i18n"
		]
	},
	"manifest_version": 2
}
