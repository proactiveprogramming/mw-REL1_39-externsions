@ARTICLE{ahmed09b,
  author = {Nayaz Khalid Ahmed and Martin Hecht},
  title = {A Lattice Boltzmann study of flow along patterned surfaces and through
	channels with alternating slip length},
  journal = {accepted},
  year = {2010},
  volume = {accepted as a contribution to the ICCMS 2010, Sanya, China, January
	22-24},
  abstract = {In microfluidics, varying wetting properties can be used to influence
	the flow of a liquid through a device. Different wetting properties
	can be expressed in terms of the local slip length at the fluid-solid
	boundary. We use a boundary condition which allows to adjust the
	slip length locally. We study flow past surfaces on which the slip
	length is modulated in stripes. We find that the effective slip length
	for such a flow can be expressed as a function of the individual
	slip lengths on the stripes. The angle dependence of the effective
	slip on a patterned surface is in excellent agreement with a recent
	theory describing the slip length as a tensorial quantity. We show
	that the tensorial nature of the effective slip length allows to
	induce a transverse flow, which can be used in micro mixers to drive
	a vortex. In our simulations of a flow through a square channel with
	patterned surfaces we see a homogeneous rotation about the direction
	of the flow which we quantify by the component of the curl in the
	direction of the flow, which appears to be nearly constant in the
	center of the channel. We investigate the influence of patterns of
	cosine shaped varying local slip on the flow field depending on the
	orientation, period and and amplitude of the pattern and find the
	largest effective slip length for large amplitudes and large periods
	of stripes parallel to the flow direction.},
  e-print = {0910.2637v1},
  file = {ahmed09b.pdf:ahmed09b.pdf:PDF},
  owner = {hecht},
  timestamp = {2009.10.15}
}

