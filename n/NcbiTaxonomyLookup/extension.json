{
	"name": "NCBITaxonomyLookup",
	"author": "WikiTeq",
	"url": "https://www.mediawiki.org/wiki/Extension:NCBITaxonomyLookup",
	"descriptionmsg": "ncbitaxonomylookup-desc",
	"license-name": "GPL-2.0-or-later",
	"version": "1.0",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.31.0"
	},
	"AutoloadClasses": {
		"NCBITaxonomyLookup\\NCBITaxonomyLookupHooks": "includes/NCBITaxonomyLookupHooks.php",
		"NCBITaxonomyLookup\\NCBITaxonomyLookupCache": "includes/NCBITaxonomyLookupCache.php",
		"NCBITaxonomyLookup\\NCBITaxonomyLookup": "includes/NCBITaxonomyLookup.php"
	},
	"config": {
		"NCBITaxonomyLookupApiURL": {
			"value": "https://eutils.ncbi.nlm.nih.gov/entrez/eutils/efetch.fcgi"
		},
		"NCBITaxonomyLookupCacheTTL": {
			"value": 0
		},
		"NCBITaxonomyLookupCacheRandomizeTTL": {
			"value": false
		},
		"NCBITaxonomyApiTimeoutFallbackToCache": {
			"value": false
		},
		"NCBITaxonomyApiTimeout": {
			"value": 60
		},
		"NCBITaxonomyApiKey": {
			"value": false
		}
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"NCBITaxonomyLookup\\NCBITaxonomyLookupHooks::onParserFirstCallInit"
		]
	},
	"MessagesDirs": {
		"NCBITaxonomyLookup": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"NCBITaxonomyLookupMagic": "NCBITaxonomyLookup.i18n.magic.php"
	},
	"manifest_version": 2
}
